{
  "name": "testserver",
  "version": "1.0.0",
  "description": "Test Server for test webpage",
  "main": "src/root.js",
  "scripts": {
    "start": "webpack-dev-server",
    "json_server": "node jsonServer.js && PORT=3000",
    "static-server": "http-server ./src/build -p 3000",
    "webpack_dev": "webpack --color=true && npm run static-server",
    "webpack_prod": "NODE_ENV='production' webpack",
    "webpack_watch": "watchMode=true webpack --color=true",
    "webpack_live": "livereload=true npm run webpack_watch",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Amar Sandhu",
  "license": "ISC",
